            <!--  Segudna tabela.  -->
            <table class="table table-bordered border-secondary table-sm">
                <tr class="tabela">
                    <td colspan="8" class="texto-central">Medição de amostra</td>
                </tr>
                <tr>
                    <td colspan="2"> Código da curva : </td>
                    <td colspan="2"> <input type="text" name="cod_curva" id="cod_curva" class="form-control input " /> </td>
                    <td colspan="2"> Fator Calibração: </td>
                    <td colspan="2"> <input type="text" name="fator_calibracao" id="fator_calibracao" class="form-control input " /> </td>
            </table>
            <br />

            <!--  Terceira tabela.  -->
            <table class="table table-bordered border-secondary table-sm">
                <tr class="tabela">
                    <td class="texto-central">Item</td>
                    <td class="texto-central">Massa (g)</td>
                    <td class="texto-central">Absorbância</td>
                    <td class="texto-central">Resultado final Máximo permitido 0,5 mg/kg</td>
                </tr>
                <tr>
                    <td>Branco Ensaio</td>
                    <td><input class="form-control input" value="---" readonly style="height:50px; text-align:center" /></td>
                    <td><input type="text" class="form-control input " name="absorvancia1" id="absorvancia1" style="height:50px" /> </td>
                    <td rowspan="2"><input class="form-control input" style="height:100px" readonly /></td>
                </tr>
                <tr>
                    <td>Amostra</td>
                    <td><input type="text" name="massa" id="massa" class="form-control input " /></td>
                    <td><input type="text" name="absorvancia2" id="absorvancia2" class="form-control input " /></td>
                </tr>
            </table>
            <br />

