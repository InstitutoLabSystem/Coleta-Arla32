                    <td colspan="1"><input type="text" name="mat_lote_1" id="mat_lote_1" class="form-control input" value="@Model.fosfato.mat_lote_1" /></td>
                    @if (Model.fosfato.mat_validade_1 != DateOnly.MinValue)
                    {
                        <td colspan="1"><input type="datetime" name="mat_validade_1" id="mat_validade_1" class="form-control input" value="@Model.fosfato.mat_validade_1" /></td>
                    }
