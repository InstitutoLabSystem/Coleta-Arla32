                    <td colspan="2"> <input type="text" name="cod_curva" id="cod_curva" class="form-control input " value="@Model.fosfato.cod_curva" /> </td>
                    <td colspan="2"> Fator Calibração: </td>
                    <td colspan="2"> <input type="text" name="fator_calibracao" id="fator_calibracao" class="form-control input " value="@Model.fosfato.fator_calibracao" /> </td>
            </table>
